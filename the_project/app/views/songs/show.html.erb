<p>
  <strong>Title:</strong>
  <%= @song.title %>
  <br>
  <% if @song.performer_id != nil %>
    <% perf_id = @song.performer_id %>
    <strong>Performer: </strong><%= Performer.find(perf_id).name %>
  <% end %>
</p>
<% if current_user.admin? %>
  <%= link_to 'Edit', edit_song_path(@song) %> |
<% end %>
<%= link_to 'Back', songs_path %>
